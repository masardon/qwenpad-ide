<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android" id="xyz.kabarbaik.qwenpad.git" version="1.0.0">
    <name>QwenPad Git</name>
    <description>Git plugin for QwenPad IDE</description>
    <license>MIT</license>
    <keywords>cordova,git</keywords>
    <repo>https://github.com/your-repo/qwenpad-ide.git</repo>

    <js-module name="Git" src="www/git.js">
        <clobbers target="window.Git" />
    </js-module>

    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="Git">
                <param name="android-package" value="xyz.kabarbaik.qwenpad.git.Git" />
            </feature>
        </config-file>

        <source-file src="src/android/Git.java" target-dir="src/xyz/kabarbaik/qwenpad/git" />
    </platform>
</plugin>